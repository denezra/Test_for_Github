package {	import flash.display.MovieClip;	import flash.geom.Point;	import flash.geom.Vector3D;	public class Confetti extends MovieClip	{		private var speed:Point;		private var rotator:Vector3D;		private const SPEED_RANGE = 8;		private const ROTATION_RANGE = 50;		var colors:Array = ["0xb30b21","0x13523e","0x004d8f","0xead350","0xeff3f3","0x5b4d4e","0x7a6e5b","0x4e555c","0x1b2024"];		public function Confetti(sx:int, sy:int)		{			x = sx;			y = sy;			//adjust your taste			speed = new Point(SPEED_RANGE/2.4-Math.random()*SPEED_RANGE, SPEED_RANGE/2.4+Math.random()*SPEED_RANGE);			rotator = new Vector3D(ROTATION_RANGE/2-Math.random()*ROTATION_RANGE,			   ROTATION_RANGE/2-Math.random()*ROTATION_RANGE,			   ROTATION_RANGE/2-Math.random()*ROTATION_RANGE);			graphics.beginFill(colors [randomRange(0, colors.length)]);			graphics.drawRect(-speed.y, -speed.y, speed.y, speed.y);			graphics.endFill();			function randomRange(minNum:Number, maxNum:Number):Number			{				return (Math.round(Math.floor(Math.random() * (maxNum - minNum + 1)) + minNum));			}		}		public function update():void		{			x +=  speed.x;			y +=  speed.y;			rotationX +=  rotator.x;			rotationY +=  rotator.y;			rotationZ +=  rotator.z;		}	}}
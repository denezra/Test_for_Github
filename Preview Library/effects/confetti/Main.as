package {	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.TimerEvent;	import flash.utils.Timer;	public class Main extends MovieClip {		private var confetti: Array;		private const CONFETTI_PER_FRAME = 1;		public function Main() {			confetti = new Array();			addEventListener(Event.ENTER_FRAME, loop);		}		private var frame: int = 0;		private var frameLimit: int = 30 * 15; //210; //Max limit: 30 * 15;		private function loop(e: Event): void {			frame++;						//if (frame < 75)			//				return;			if (confetti.length > 0)				for (i = confetti.length - 1; i >= 0; i--) {					confetti[i].update();					if (confetti[i].y > stage.stageHeight + 20) {						removeChild(confetti[i]);						confetti.splice(i, 1);					}				}			if (frame < frameLimit - 100) {				for (var i: int = 0; i < CONFETTI_PER_FRAME; i++) {					var c: Confetti = new Confetti(300 * Math.random(), -20);					confetti.push(c);					addChild(c);					//this.setChildIndex(this.numChildren - 4);				}			}			if (frame > frameLimit)				removeEventListener(Event.ENTER_FRAME, loop);		}	}}